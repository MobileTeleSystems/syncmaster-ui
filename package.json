{
  "name": "syncmaster_ui",
  "version": "0.3.1",
  "private": true,
  "description": "Frontend for SyncMaster",
  "license": "Apache-2.0",
  "packageManager": "yarn@4.8.1",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve",
    "prod": "cross-env NODE_ENV=production webpack serve",
    "build": "rimraf ./dist && cross-env NODE_OPTIONS=--max_old_space_size=8192 && cross-env NODE_ENV=production webpack",
    "lint": "eslint --fix src",
    "lint-check": "eslint src",
    "type-check": "tsc --noEmit",
    "prettier": "prettier src --write --list-different",
    "prettier-check": "prettier src --check --list-different",
    "bundle-analyze": "cross-env BUNDLE_ANALYZER=true cross-env NODE_ENV=production webpack --progress",
    "check": "yarn lint && yarn type-check",
    "pre-commit-install": "husky"
  },
  "dependencies": {
    "@ant-design/icons": "5.6.1",
    "@tanstack/react-query": "5.90.14",
    "@tanstack/react-query-devtools": "5.91.2",
    "@xyflow/react": "12.10.0",
    "antd": "4.24.16",
    "axios": "1.13.2",
    "clsx": "2.1.1",
    "dayjs": "1.11.19",
    "dotenv-webpack": "8.1.1",
    "i18next": "24.2.3",
    "rc-picker": "4.11.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.1.2",
    "react-i18next": "15.7.4",
    "react-router-dom": "6.30.2"
  },
  "devDependencies": {
    "@svgr/webpack": "8.1.0",
    "@types/node": "20.19.27",
    "@types/react": "18.3.27",
    "@types/react-dom": "18.3.7",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "cross-env": "7.0.3",
    "css-loader": "6.11.0",
    "css-minimizer-webpack-plugin": "6.0.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "4.6.2",
    "fork-ts-checker-webpack-plugin": "9.1.0",
    "html-webpack-plugin": "5.6.5",
    "husky": "9.1.7",
    "less": "4.5.1",
    "less-loader": "12.3.0",
    "lint-staged": "15.5.2",
    "mini-css-extract-plugin": "2.9.4",
    "prettier": "3.7.4",
    "rimraf": "5.0.10",
    "style-loader": "3.3.4",
    "terser-webpack-plugin": "5.3.16",
    "ts-loader": "9.5.4",
    "typescript": "5.9.3",
    "webpack": "5.104.1",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.2.2",
    "webpack-merge": "5.10.0"
  },
  "engines": {
    "node": ">18",
    "npm": ">10"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css,less}": "prettier --write --list-different",
    "src/**/*.{ts,tsx}": "eslint --fix"
  }
}
