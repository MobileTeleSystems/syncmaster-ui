{
  "name": "syncmaster_ui",
  "version": "0.2.0",
  "private": true,
  "description": "Frontend for SyncMaster",
  "license": "Apache-2.0",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve",
    "start:prod": "cross-env NODE_ENV=production webpack serve",
    "build": "rimraf ./dist && cross-env NODE_OPTIONS=--max_old_space_size=8192 && cross-env NODE_ENV=production webpack",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "check": "yarn lint && yarn type-check",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "bundle:analyze": "cross-env BUNDLE_ANALYZER=true cross-env NODE_ENV=production webpack --progress",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons": "5.6.1",
    "@tanstack/react-query": "5.71.5",
    "@tanstack/react-query-devtools": "5.71.5",
    "@xyflow/react": "12.5.4",
    "antd": "4.24.16",
    "axios": "1.8.4",
    "clsx": "2.1.1",
    "dayjs": "1.11.13",
    "dotenv-webpack": "8.1.0",
    "i18next": "24.2.3",
    "rc-picker": "4.11.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.1.2",
    "react-i18next": "15.4.1",
    "react-router-dom": "6.30.0"
  },
  "devDependencies": {
    "@svgr/webpack": "8.1.0",
    "@types/node": "20.17.30",
    "@types/react": "18.3.20",
    "@types/react-dom": "18.3.6",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "cross-env": "7.0.3",
    "css-loader": "6.11.0",
    "css-minimizer-webpack-plugin": "6.0.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "4.6.2",
    "fork-ts-checker-webpack-plugin": "9.0.3",
    "html-webpack-plugin": "5.6.3",
    "husky": "9.1.7",
    "less": "4.2.2",
    "less-loader": "12.2.0",
    "lint-staged": "15.5.0",
    "mini-css-extract-plugin": "2.9.2",
    "prettier": "3.5.3",
    "rimraf": "5.0.10",
    "style-loader": "3.3.4",
    "terser-webpack-plugin": "5.3.14",
    "ts-loader": "9.5.2",
    "typescript": "5.8.2",
    "webpack": "5.98.0",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.2.1",
    "webpack-merge": "5.10.0"
  },
  "lint-staged": {
    "*.(ts|tsx|js|jsx)": [
      "node tsc-check.js",
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">18",
    "npm": ">10"
  },
  "packageManager": "yarn@4.8.1"
}
